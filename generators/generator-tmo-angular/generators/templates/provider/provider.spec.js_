'use strict';
describe('Provider: <%= name %>Provider', function() {
  var provider;

  beforeEach(function () {
      module('<%= appName %>');
      inject(function(_<%= name %>_) {
          provider = _<%= name %>_;
      });
  });
  it('should return the name', function () {
      expect(provider.getName()).to.equal('This is <%= name %>Provider.');
  });
});