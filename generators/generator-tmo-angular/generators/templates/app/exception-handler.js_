'use strict';
/**
 * @ngdoc module
 * @file exception-handler.js
 * @description # <%= appName %> exception-handler module loader
 */

angular.module('<%= appName %>').factory('$exceptionHandler', function () {
	return function (exception, cause) {
		console.error(exception + ' (caused by "' + cause + '")');
		//Place an HTTP call back to the server here to report the message!
	};
});